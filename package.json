  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "start:dev": "NODE_ENV=development webpack && nodemon build/index.js",
    "ts-node": "node --loader ts-node/esm node_modules/.bin/webpack --config webpack.config.ts",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' webpack --mode development && nodemon build/index.js",
    "dev2": "NODE_OPTIONS='--loader ts-node/esm' npx webpack --entry ./build/index.js --mode development && nodemon build/index.js"
  },
